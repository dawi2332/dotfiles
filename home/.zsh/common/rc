setopt autoresume completealiases correct
unsetopt correctall

watch=(all)

path=($HOME/.bin /usr/local/bin $path)
fpath=($HOME/.zsh/functions $fpath)
cdpath=(. .. ~ $cdpath)

MANPATH=${MANPATH:-`manpath`}
export MANPATH

bindkey '^R' history-incremental-search-backward

